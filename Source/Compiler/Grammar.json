{
  "statement-list": [
    [ "statement-list", ";", "statement-list" ],
    [ "statement" ],
    [ "statement", ";" ]
  ],
  "statement": [
    [ "if-statement" ],
    [ "while-statement" ],
    [ "do-while-statement" ],
    [ "foreach-statement" ],
    [ "fork-statement" ],
    [ "return-statement" ],
    [ "yield-statement" ],
    [ "function-definition" ],
    [ "expression" ]
  ],
  "if-statement": [
    [ "if", "(", "expression", ")", "embedded-statement" ],
    [ "if", "(", "expression", ")", "embedded-statement", "else", "embedded-statement" ]
  ],
  "while-statement": [
    [ "while", "(", "expression", ")", "embedded-statement" ]
  ],
  "do-while-statement": [
    [ "do", "embedded-statement", "while", "(", "expression", ")" ],
    [ "do", "embedded-statement", "while", "(", "expression", ")", "embedded-statement" ]
  ],
  "foreach-statement": [
    [ "foreach", "(", "identifier", "in", "expression", ")", "embedded-statement" ]
  ],
  "fork-statement": [
    [ "fork", "embedded-statement" ],
    [ "fork", "embedded-statement", "else", "embedded-statement" ]
  ],
  "return-statement": [
    [ "return", "expression" ]
  ],
  "yield-statement": [
    [ "yield" ]
  ],
  "function-definition": [
    [ "func", "identifier", "(", "identifier-list", ")", "block" ]
  ],
  "list-manipulation-expression": [
    [ "rpush", "(", "identifier", ",", "expression", ")" ],
    [ "lpush", "(", "identifier", ",", "expression", ")" ],
    [ "rpop", "(", "identifier", ")" ],
    [ "lpop", "(", "identifier", ")" ]
  ],
  "identifier-list": [
    [ "identifier-list", ",", "identifier-list" ],
    [ "identifier" ]
  ],
  "embedded-statement": [
    [ "expression" ],
    [ "return-statement" ],
    [ "block" ]
  ],
  "block": [
    [ "{", "}" ],
    [ "{", "statement-list", "}" ]
  ],
  "expression": [
    [ "literal" ],
    [ "identifier" ],
    [ "list-manipulation-expression" ],
    [ "function-call" ],
    [ "wrapped-expression" ],
    [ "binary-expression" ],
    [ "unary-expression" ],
    [ "get-field-expression" ],
    [ "set-field-expression" ],
    [ "del-field-expression" ]
  ],
  "get-field-expression": [
    [ "identifier", "[", "expression", "]"]
  ],
  "set-field-expression": [
    [ "identifier", "[", "expression", "]", "=", "expression" ]
  ],
  "del-field-expression": [
    [ "del", "identifier", "[", "expression", "]"]
  ],
  "binary-expression": [
    [ "operand", "=", "operand" ],
    [ "operand", "==", "operand" ],
    [ "operand", "<", "operand" ],
    [ "operand", ">", "operand" ],
    [ "operand", "<=", "operand" ],
    [ "operand", ">=", "operand" ],
    [ "operand", "and", "operand" ],
    [ "operand", "or", "operand" ],
    [ "operand", "xor", "operand" ],
    [ "operand", "+", "operand" ],
    [ "operand", "-", "operand" ],
    [ "operand", "*", "operand" ],
    [ "operand", "/", "operand" ]
  ],
  "unary-expression": [
    [ "left-unary-expression" ],
    [ "right-unary-expression" ]
  ],
  "left-unary-expression": [
    [ "-", "operand" ],
    [ "~", "operand" ],
    [ "!", "operand" ]
  ],
  "right-unary-expression": [
    [ "operand", "!" ]
  ],
  "wrapped-expression": [
    [ "(", "expression", ")" ]
  ],
  "operand": [
    [ "literal" ],
    [ "identifier" ],
    [ "function-call" ],
    [ "expression" ]
  ],
  "literal": [
    [ "string-literal" ],
    [ "number-literal" ],
    [ "list-literal" ]
  ],
  "list-literal": [
    [ "[", "]" ],
    [ "[", "list-literal-element-list", "]" ]
  ],
  "list-literal-element-list": [
    [ "list-literal-element-list", ",", "list-literal-element-list" ],
    [ "expression" ]
  ],
  "function-call": [
    [ "identifier", "(", ")" ],
    [ "identifier", "(", "argument-list", ")" ]
  ],
  "argument-list": [
    [ "argument-list", ",", "argument-list" ],
    [ "argument" ]
  ],
  "argument": [
    [ "expression" ]
  ]
}