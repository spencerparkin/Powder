{
  "statement-list": [
    [ "statement-list", ";", "statement-list" ],
    [ "statement" ],
    [ "statement", ";" ]
  ],
  "statement": [
    [ "if-statement" ],
    [ "while-statement" ],
    [ "do-while-statement" ],
    [ "foreach-statement" ],
    [ "fork-statement" ],
    [ "return-statement" ],
    [ "yield-statement" ],
    [ "function-definition" ],
    [ "expression" ]
  ],
  "if-statement": [
    [ "if", "(", "expression", ")", "embedded-statement" ],
    [ "if", "(", "expression", ")", "embedded-statement", "else", "embedded-statement" ]
  ],
  "while-statement": [
    [ "while", "(", "expression", ")", "embedded-statement" ]
  ],
  "do-while-statement": [
    [ "do", "embedded-statement", "while", "(", "expression", ")" ],
    [ "do", "embedded-statement", "while", "(", "expression", ")", "embedded-statement" ]
  ],
  "foreach-statement": [
    [ "foreach", "(", "identifier", "in", "expression", ")", "embedded-statement" ]
  ],
  "fork-statement": [
    [ "fork", "embedded-statement" ],
    [ "fork", "embedded-statement", "else", "embedded-statement" ]
  ],
  "return-statement": [
    [ "return", "expression" ]
  ],
  "yield-statement": [
    [ "yield" ]
  ],
  "function-definition": [
    [ "func", "identifier", "(", "identifier-list", ")", "block" ]
  ],
  "identifier-list": [
    [ "identifier-list", ",", "identifier-list" ],
    [ "identifier" ]
  ],
  "embedded-statement": [
    [ "expression" ],
    [ "return-statement" ],
    [ "block" ]
  ],
  "block": [
    [ "{", "}" ],
    [ "{", "statement-list", "}" ]
  ],
  "expression": [
    [ "literal" ],
    [ "identifier" ],
    [ "wrapped-expression" ],
    [ "membership-expression" ],
    [ "container-size-expression" ],
    [ "list-push-pop-expression" ],
    [ "binary-expression" ],
    [ "unary-expression" ],
    [ "get-field-expression" ],
    [ "set-field-expression" ],
    [ "del-field-expression" ],
    [ "function-call" ]
  ],
  "membership-expression": [
    [ "expression", "in", "expression" ]
  ],
  "list-push-pop-expression": [
    [ "expression", "-->", "expression" ],
    [ "expression", "<--", "expression" ],
    [ "expression", "--<", "expression" ],
    [ "expression", ">--", "expression" ]
  ],
  "container-size-expression": [
    [ "len", "(", "expression", ")" ]
  ],
  "map-keys-expression": [
    [ "keys", "(", "expression", ")" ]
  ],
  "get-field-expression": [
    [ "identifier", "[", "expression", "]" ]
  ],
  "set-field-expression": [
    [ "identifier", "[", "expression", "]", "=", "expression" ]
  ],
  "del-field-expression": [
    [ "del", "identifier", "[", "expression", "]" ]
  ],
  "binary-expression": [
    [ "expression", "=", "expression" ],
    [ "expression", "==", "expression" ],
    [ "expression", "<", "expression" ],
    [ "expression", ">", "expression" ],
    [ "expression", "<=", "expression" ],
    [ "expression", ">=", "expression" ],
    [ "expression", "&&", "expression" ],
    [ "expression", "||", "expression" ],
    [ "expression", "+", "expression" ],
    [ "expression", "-", "expression" ],
    [ "expression", "*", "expression" ],
    [ "expression", "/", "expression" ]
  ],
  "unary-expression": [
    [ "left-unary-expression" ],
    [ "right-unary-expression" ]
  ],
  "left-unary-expression": [
    [ "-", "expression" ],
    [ "~", "expression" ],
    [ "!", "expression" ]
  ],
  "right-unary-expression": [
    [ "expression", "!" ]
  ],
  "wrapped-expression": [
    [ "(", "expression", ")" ]
  ],
  "literal": [
    [ "string-literal" ],
    [ "number-literal" ],
    [ "list-literal" ],
    [ "map-literal" ]
  ],
  "list-literal": [
    [ "[", "]" ],
    [ "[", "list-element-list", "]" ]
  ],
  "list-element-list": [
    [ "list-element-list", ",", "list-element-list" ],
    [ "expression" ]
  ],
  "map-literal": [
    [ "{", "}" ],
    [ "{", "map-pair-list", "}" ]
  ],
  "map-pair-list": [
    [ "map-pair-list", ",", "map-pair-list" ],
    [ "map-pair" ]
  ],
  "map-pair": [
    [ "expression", ":", "expression" ]
  ],
  "function-call": [
    [ "identifier", "(", ")" ],
    [ "identifier", "(", "argument-list", ")" ]
  ],
  "argument-list": [
    [ "argument-list", ",", "argument-list" ],
    [ "argument" ]
  ],
  "argument": [
    [ "expression" ]
  ]
}