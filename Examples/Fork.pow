# Fork.pow

proc main()
{
    fork
    {
        i = 0;
        loop
        {
            print("fork A: " + i);
            yield;
            i = i + 1;
        }
    }
    else
    {
        i = 0;
        loop
        {
            print("fork B: " + i);
            yield;
            i = i - 1;
        }
    }
}