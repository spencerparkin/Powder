# ListTests.pow

list = [1, 1 + 1, 1 + 1 + 1];

func blah(given_list)
{
    given_list --< 4;
    0 >-- given_list;
};

blah(list);

i <-- list;
list --> j;

output("i = " + i + "\n");
output("j = " + j + "\n");

func print_list(given_list)
{
    i = 0;
    while(i < len(given_list))
    {
        output(i + ": " + given_list[i] + "\n");
        i = i + 1;
    }
};

output("len(list) = " + len(list) + "\n");
print_list(list);